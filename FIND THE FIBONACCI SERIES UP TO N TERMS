#include <stdio.h>

int main() {
    int n, first = 0, second = 1, next;

    // terms ka number dalo
    printf("Enter the number of terms: ");
    scanf("%d", &n);

    // Check karo valid input 
    if (n <= 0) {
        printf("Please enter a positive integer.\n");
    } else {
        printf("Fibonacci series up to %d terms:\n", n);

        for (int i = 1; i <= n; i++) {
            printf("%d ", first);
            next = first + second;
            first = second;
            second = next;
        }
        printf("\n");
    }

    return 0;
}
