#include <stdio.h>

int main() {
    int num;

    // ek number dalo
    printf("Enter a number to print its multiplication table: ");
    scanf("%d", &num);

    // multiplication table print karo
    printf("Multiplication table of %d:\n", num);
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i);
    }

    return 0;
}
